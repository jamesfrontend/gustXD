{
  "name": "@labxd/gustxd",
  "module": "dist/gustxd.esm.js",
  "description": "UI react components with Tailwind CSS in mind.",
  "version": "0.4.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "presets.js",
    "README.md"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./styles": "./dist/gustxd.cjs.production.min.css",
    "./tailwind": "./presets.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "pnpm sb",
    "watch": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "sb": "start-storybook -p 6006",
    "build:sb": "build-storybook",
    "prepublishOnly": "tsdx build",
    "prepublish:sb": "pnpm build:sb",
    "publish:sb": "pnpm dlx chromatic --project-token=${CHROMATIC_TOKEN}"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@size-limit/preset-small-lib": "7.0.8",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.5.9",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/react": "6.5.9",
    "@storybook/theming": "6.5.9",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "babel-loader": "8.2.5",
    "chromatic": "6.6.3",
    "cssnano": "^5.1.12",
    "husky": "8.0.1",
    "postcss": "8.4.14",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "rimraff": "0.0.1-security",
    "rollup-plugin-postcss": "4.0.2",
    "size-limit": "7.0.8",
    "storybook-css-modules-preset": "1.1.1",
    "tailwindcss": "3.1.3",
    "tsdx": "0.14.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "storybook-dark-mode": "^1.1.0",
    "@storybook/addon-docs": "6.5.9",
    "clsx": "^1.2.1"
  },
  "size-limit": [
    {
      "path": "dist/gustxd.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gustxd.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@tailwindcss/forms": "^0.5.2"
  }
}
