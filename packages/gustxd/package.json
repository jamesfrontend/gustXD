{
  "name": "@labxd/gustxd",
  "module": "dist/gustxd.esm.js",
  "description": "UI react components with Tailwind CSS in mind.",
  "version": "0.7.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "labXD Core",
  "contributors": [
    "Jimmy Nguyen <jimmy@labxd.com> (https://github.com/jimmynxd)",
    "Walee Ahmed <walee@labxd.com> (https://github.com/wallexd)"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/labXD/gustXD/",
    "directory": "packages/gustxd"
  },
  "files": [
    "dist",
    "presets.js",
    "README.md",
    "src/styles/**",
    "src/plugin/**",
    "src/colors.js"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./styles-standalone.css": "./dist/gustxd.cjs.production.min.css",
    "./styles.css": "./src/styles/globals.css",
    "./colors": "./src/colors.js",
    "./tailwind": "./presets.js",
    "./tailwind-preset": "./presets.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "pnpm sb",
    "watch": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "sb": "start-storybook -p 6006",
    "build:sb": "build-storybook",
    "prepublishOnly": "tsdx build",
    "prepublish:sb": "pnpm build:sb",
    "publish:sb": "pnpm dlx chromatic --project-token=${CHROMATIC_TOKEN}"
  },
  "peerDependencies": {
    "@headlessui/react": ">=1.7.0",
    "react": ">=17"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@headlessui/react": "^1.7.0",
    "@size-limit/preset-small-lib": "8.0.1",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/theming": "^6.5.12",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.8",
    "babel-loader": "8.2.5",
    "chromatic": "6.8.0",
    "clsx": "^1.2.1",
    "cssnano": "^5.1.13",
    "husky": "8.0.1",
    "postcss": "8.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "rimraff": "0.0.1-security",
    "rollup-plugin-postcss": "4.0.2",
    "size-limit": "8.0.1",
    "storybook-css-modules-preset": "1.1.1",
    "storybook-dark-mode": "^1.1.2",
    "tailwindcss": "3.1.8",
    "tsdx": "0.14.1",
    "tslib": "2.4.0",
    "typescript": "4.8.2",
    "webpack": "5.74.0"
  },
  "size-limit": [
    {
      "path": "dist/gustxd.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gustxd.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "mini-svg-data-uri": "^1.4.4"
  }
}
